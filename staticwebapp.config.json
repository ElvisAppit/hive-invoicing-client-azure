{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/",
      "statusCode": 301
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}
